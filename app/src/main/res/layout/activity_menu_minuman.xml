<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuMinuman">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="match_parent"
            android:layout_height="194dp"
            app:srcCompat="@drawable/minumm" />

        <Button
            android:id="@+id/btnLemonTea"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="#FF5D51"
            android:text="Tea Jasmine"
            android:textColor="@color/colorAccent" />

        <Button
            android:id="@+id/btnMojitoLeci"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="#FF5D51"
            android:text="Mojito Strawberry"
            android:textColor="@color/colorAccent" />

        <Button
            android:id="@+id/btnMilkshake"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="#FF5D51"
            android:text="Milkshake Oreo"
            android:textColor="@color/colorAccent" />

        <Button
            android:id="@+id/btnMatchaLatte"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="#FF5D51"
            android:text="Matcha Frappucino"
            android:textColor="@color/colorAccent" />

        <Button
            android:id="@+id/btnTaroMilk"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="#FF5D51"
            android:text="Taro Milk Ice"
            android:textColor="@color/colorAccent" />

    </LinearLayout>



</RelativeLayout>